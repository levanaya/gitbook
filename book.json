{
  "title": "hamster使用文档",
  "description": "hamster使用手册",
  "language": "en",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "links": {},
  "plugins": [ "splitter", "intopic-toc", "-highlight", "prism", "prism-themes","back-to-top-button"],
