{
  "title": "hamster使用文档",
  "description": "hamster使用手册",
  "language": "en",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "links": {},
  "plugins": [
    "splitter",
    "intopic-toc",
    "-highlight",
    "prism",
    "prism-themes",
    "back-to-top-button",
    "-sharing",
    "scripts"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "fontsettings": {
      "theme": "night",
      "family": "sans",
      "size": 2
    },
    "intopic-toc": {
      "label": {
        "de": "In diesem Artikel",
        "en": "In this article",
        "zh": "本篇章节"
      }
    },
    "prism": {
      "css": ["prism-themes/themes/prism-duotone-dark.css"]
    },
    "links": [
      {
        "name": "Github",
        "icon": "fa-brands fa-github",
        "url": "https://github.com/hamster-shared"
      },
      {
        "name": "Twitter",
        "icon": "fa-brands fa-twitter",
        "url": "https://twitter.com/Hamsternetio"
      },
      {
        "name": "Discord",
        "icon": "fa-brands fa-discord",
        "url": "https://discord.gg/MrJWxRwXpb"
      },
      {
        "name": "Medium",
        "icon": "fa-brands fa-medium",
        "url": "https://medium.com/@Hamsternetio"
      }
    ],
    "scripts": {
      "files": ["./javascripts/links.js"]
    }
  }
}
